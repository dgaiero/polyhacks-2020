{
  "expo": {
    "name": "sugr",
    "slug": "sugr",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
       "package": "com.sugr.sugrApp"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#31393C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBarColor": "#2176FF",
    "androidStatusBar": {
       "barStyle": "light-content",
       "backgroundColor": "#2176FF"
    }
  }
}
